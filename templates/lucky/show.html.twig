{% extends 'base.html.twig' %}

{% block body %}
    <h1 style="margin: 80px 0 20px;">Film</h1>

    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ film.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ film.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ film.description }}</td>
            </tr>
             
            <tr>
                <th>Yearpublication</th>
                <td>{{ film.yearPublication }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
