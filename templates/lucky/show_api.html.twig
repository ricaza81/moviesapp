{% extends 'base.html.twig' %}

{% block body %}
    <h1 style="margin: 80px 0 20px;">Api Film</h1>

    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Title</th>
                <td>{{ film.title }}</td>
            </tr>
            <tr>
                <th>Yearpublication</th>
